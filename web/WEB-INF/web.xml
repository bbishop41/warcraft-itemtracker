<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>parseLoot</servlet-name>
        <servlet-class>parseLoot.parseLoot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchLootByMember</servlet-name>
        <servlet-class>SearchHandlers.searchLootByMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insertMember</servlet-name>
        <servlet-class>MemberServlets.insertMember</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>processLogin</servlet-name>
        <servlet-class>loginServlets.processLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>parseLoot</servlet-name>
        <url-pattern>/parseLoot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchLootByMember</servlet-name>
        <url-pattern>/searchLootByMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insertMember</servlet-name>
        <url-pattern>/insertMember</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processLogin</servlet-name>
        <url-pattern>/processLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <description>DB Connection</description>
        <res-ref-name>jdbc/obsidiumloot</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
